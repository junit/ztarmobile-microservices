<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"

       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.3.xsd

       http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-4.3.xsd" xmlns:util="http://www.springframework.org/schema/util">
    
    <util:properties id="cdrFileDaoSql">
        <prop key="select.cdr_file">
            <![CDATA[
                SELECT row_id, source_file_name, target_file_name
                FROM   invoicing_cdr_file
                WHERE  source_file_name = :file_name
            ]]>
        </prop>
        <prop key="insert.cdr_file">
            <![CDATA[
                INSERT INTO invoicing_cdr_file (source_file_name, target_file_name, file_type)
                VALUES (:source_file_name, :target_file_name, :file_type)
                ON DUPLICATE KEY 
                UPDATE load_date = NOW()
            ]]>
        </prop>
    </util:properties>
    
</beans>
